<%- include("partial/header") %>
        <aside>
              <table class="table table-hover">
                <thead>
                  <tr>
                    <th scope="col">Image</th>
                    <th scope="col">Name</th>
                    <th scope="col">Position</th>
                    <th scope="col">Action</th>
                  </tr>
                </thead>
                <tbody>
                    <% voters_list.forEach(voter => { %>
                  <tr>
                    <td><img src="" alt="" class="voter-img"></td>
                    <td><%= voter.firstname %> <%= voter.middlename %> <%= voter.lastname %></td>
                    <td>Voter</td>
                    <% if (voter.voted == "TRUE") {%>
                      <td>
                        <form action="/vote_candidate?<%= voter.id %>" method="get">
                          <input type="hidden" name="voter_id" value="<%= voter.id %>">
                          <input type="submit" value="Voted" class="btn btn-success disabled">
                        </form>
                      </td>
                    <% } else { %>
                      <td>
                        <form action="/vote_candidate?<%= voter.id %>" method="get">
                          <input type="hidden" name="voter_id" value="<%= voter.id %>">
                          <input type="submit" value="Vote" class="btn btn-primary">
                        </form>
                      </td>
                      <% } %>
                  </tr>
                  <% }); %>
                </tbody>
              </table>
        </aside>
        <%- include("partial/footer") %>